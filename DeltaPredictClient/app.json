{
  "expo": {
    "name": "DeltaPredictClient",
    "slug": "DeltaPredictClient",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Photos/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/Photos/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Photos/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    
    },
    "web": {
      "favicon": "./assets/Photos/favicon.png"
  
    }
  }
}
